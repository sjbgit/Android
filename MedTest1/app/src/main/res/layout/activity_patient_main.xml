<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sbunke.medtest1.PatientMainActivity">

    <TextView
        android:text="@string/patientMain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check In"
        android:id="@+id/patientCheckInBtn"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginTop="53dp"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/btnPatientPrescriptions" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prescriptions"
        android:id="@+id/btnPatientPrescriptions"
        android:layout_below="@+id/patientCheckInBtn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set Check In Alarms"
        android:id="@+id/btnSetCheckinAlarms"
        android:layout_below="@+id/btnPatientPrescriptions"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="53dp" />

</RelativeLayout>
